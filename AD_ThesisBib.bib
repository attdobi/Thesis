%encounter recombination 
@article{Mozumder,
title = "Free-ion yield and electron-ion recombination rate in liquid xenon ",
journal = "Chemical Physics Letters ",
volume = "245",
number = "4–5",
pages = "359 - 363",
year = "1995",
note = "",
issn = "0009-2614",
doi = "http://dx.doi.org/10.1016/0009-2614(95)01024-4",
url = "http://www.sciencedirect.com/science/article/pii/0009261495010244",
author = "A. Mozumder",
abstract = "A recently proposed re-encounter model for free-ion yeild at low-LET is applied to liquid xenon. For an initial electron-ion separation given by the thermalization distance (≈ 4000–5000 nm), good agreement with measured escape probability is obtained for an encounter recombination probability ≈0.01–0.03. Analysis of homogeneous recombination at low external fields places the electron-ion recombination rate in the range of (4.0–8.5) × 1015 M−1 s−1. "
}

